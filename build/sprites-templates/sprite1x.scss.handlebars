.sprite1x{
	display:inline-block;
	overflow: hidden;
	background-repeat: no-repeat;
	background-image: url('../img/1x/{{spritesheet.escaped_image}}');
}

{{#sprites}}
.sprite1x-{{name}}{
	@extend .sprite1x;
	width: {{px.width}}; 
	height: {{px.height}}; 
	background-position: {{px.offset_x}} {{px.offset_y}};
}

{{/sprites}}