.sprite{
	display:inline-block;
	overflow: hidden;
	background-repeat: no-repeat;
	background-image: url('../img/1x/{{spritesheet.escaped_image}}');
}

{{#sprites}}
.sprite-{{name}}{ 
	.sprite;
	width: {{px.width}}; 
	height: {{px.height}}; 
	background-position: {{px.offset_x}} {{px.offset_y}};
}

{{/sprites}}